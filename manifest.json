{
    "manifest_version":2,
    "version":"1.0.0",
    "name":"Copyright cleaner",
    "description":"Clear the copyright",
    "browser_action":{},
    "permissions":[
        "http://*/*", 
        "https://*/*"
    ],
    "background":{},
    "content_scripts":[{
            "matches":["<all_urls>"],
            "js":["index.js"],
            "run_at":"document_end",
            "all_frames":true
 
        }]
     
}